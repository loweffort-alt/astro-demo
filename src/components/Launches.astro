import { type APISpaceXResponse } from "../types/api";

const res = await fetch("https://api.spacexdata.com/v5/launches/query", {
  method: "POST",
  headers: {
    "Content-Type": "application/json",
  },
  body: JSON.stringify({
    query: {},
    option: {
      sort: {
        date_unix: "asc",
      },
      limit: 12,
    },
  }),
});

const { docs } = (await res.json()) as APISpaceXResponse;
